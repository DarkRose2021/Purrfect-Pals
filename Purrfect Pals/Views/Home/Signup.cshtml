@model LoginInfo
<h1 id="header" class="headerLightMode">Sign Up</h1>
<div class="form">
	<form asp-controller="Home" asp-action="Signup" method="post">
		<label asp-for="Username"></label>
		<input asp-for="Username" /><br />
		<label asp-for="Password"></label>
		<input asp-for="Password" /><br />
		<label asp-for="PetName"></label>
		<input asp-for="PetName" /><br />
		<button type="submit">Sign Up</button>
	</form>
</div>
